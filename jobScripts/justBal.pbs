#!/bin/sh
#PBS -l nodes=21:ppn=2
#PBS -l walltime=30:00
#PBS -j oe
#PBS

numprocs=42

inp=chk
writeO=0
incCor=1

prefixPath=/opt/openmpi/1.2.4

cd ${HOME}/otk

mpirun -prefix $prefixPath -nooversubscribe -np ${numprocs}  ./justBal $inp $writeO $incCor >& bal.${numprocs}.txt

status=$?
exit ${status}

